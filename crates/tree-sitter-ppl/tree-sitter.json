{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ppl",
      "camelcase": "PPL",
      "title": "PCBoard Programming Language",
      "scope": "source.ppl",
      "file-types": [
        "pps",
        "ppd",
        "ppx"
      ],
      "injection-regex": "^ppl$",
      "class-name": "TreeSitterPcboard"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "PPL was a BBS specific programming language.",
    "authors": [
      {
        "name": "Mike Kr√ºger",
        "email": "mkrueger@posteo.de"
      }
    ],
    "links": {
      "repository": "https://github.com/mkrueger/icy_board/"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}